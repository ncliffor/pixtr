<%= link_to "Create Group", new_group_path %>


<ul>
  <% @groups.each do |group| %>
    <li>
      <%= link_to group.name, group %>
      -
      <%= group.description %>
      (<% if group.members.include? current_user %>
        yes
      <% else%>
        no
      <% end %>)
    </li>
  <% end %>
</ul>
